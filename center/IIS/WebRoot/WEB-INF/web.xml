<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <servlet-name>usrLogLet</servlet-name>
    <servlet-class>com.servlet.usrLogLet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>usrRegLet</servlet-name>
    <servlet-class>com.servlet.usrRegLet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>dataRecLet</servlet-name>
    <servlet-class>com.servlet.dataRecLet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>dataPushLet</servlet-name>
    <servlet-class>com.servlet.dataPushLet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>dataPullLet</servlet-name>
    <servlet-class>com.servlet.dataPullLet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>deviceSyncLet</servlet-name>
    <servlet-class>com.servlet.deviceSyncLet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>usrLogLet</servlet-name>
    <url-pattern>/usrLogLet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>usrRegLet</servlet-name>
    <url-pattern>/usrRegLet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>dataRecLet</servlet-name>
    <url-pattern>/dataRecLet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>dataPushLet</servlet-name>
    <url-pattern>/dataPushLet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>dataPullLet</servlet-name>
    <url-pattern>/dataPullLet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>deviceSyncLet</servlet-name>
    <url-pattern>/deviceSyncLet</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>